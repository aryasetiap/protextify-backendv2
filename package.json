  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:email": "jest src/email/",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:cors": "node test-cors.js",
    "postbuild": "npm run copy:templates && npm run copy:assets",
    "copy:templates": "copyfiles -u 3 src/email/templates/* dist/email/templates",
    "copy:assets": "copyfiles -u 2 src/assets/* dist/src/assets",
    "queue:dashboard": "bull-board",
    "queue:clean": "node -e \"require('./dist/src/plagiarism/plagiarism.service.js').cleanupOldJobs()\"",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset && npx prisma db seed"
  },
